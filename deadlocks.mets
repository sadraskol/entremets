init do
    insert into accounts (id, balance) values (11, 0), (22, 0)
end

process do
    begin read_committed
    update accounts set balance := 100 where acctnum = 11
    update accounts set balance := 100 where acctnum = 22
    commit
end

process do
    begin read_committed
    update accounts set balance = 50 where acctnum = 22
    update accounts set balance = 50 where acctnum = 11
    commit
end

property = eventually<select balance from accounts in {{50, 50}, {100, 100}}>