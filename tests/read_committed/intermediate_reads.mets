init do
    insert into users (id, age) values (1, 10)
end

process do
    begin read_committed
    update users set age := 101 where id = 1
latch
    commit
end

process do
    begin read_committed
    let age := select age from users where id = 1
    update users set age := 11 where id = 1
    commit
end

property = never[age = 101]
property = eventually<select age from users where id = 1 is 11>
